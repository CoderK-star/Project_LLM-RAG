{
  "expo": {
    "name": "ゴミカル",
    "slug": "gomical",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gomical",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F9FC"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.gomical.app",
      "infoPlist": {
        "CFBundleDisplayName": "ゴミカル",
        "NSUserTrackingUsageDescription": "広告の最適化のために使用します"
      }
    },
    "android": {
      "package": "com.gomical.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F8F9FC"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "updates": {
      "url": ""
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
